{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Hiworks Commute",
  "version": "0.1.0",
  "identifier": "com.hiworks.commute",
  "build": {
    "frontendDist": "../src",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run serve",
    "beforeBuildCommand": ""
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../scripts/bundle": "bundle"
    },
    "macOS": {
      "minimumSystemVersion": "10.15"
    }
  },
  "plugins": {}
}
